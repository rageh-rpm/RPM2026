<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
<record id="view_country_state_list_inherit" model="ir.ui.view">
    <field name="name">res.country.state.list.inherit</field>
    <field name="model">res.country.state</field>
    <field name="inherit_id" ref="base.view_country_state_tree"/>
    <field name="arch" type="xml">

        <xpath expr="//list/field[@name='code']" position="after">
            <field name="distance_km"/>
            <field name="fixed_fare"/>
        </xpath>
    </field>
</record>




    </data>
</odoo>